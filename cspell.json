{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en,en-US",
  "allowCompoundWords": true,
  "words": [
    "antd",
    "iconify",
    "pinia",
    "unocss",
    "unplugin",
    "vueuse"
  ],
  "ignorePaths": [
    "**/node_modules/**",
    "**/dist/**",
    "**/icons/**",
    "pnpm-lock.yaml",
    "**/*.log",
    "**/*.test.ts",
    "**/__tests__/**"
  ]
}
